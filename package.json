{
  "name": "codeaid",
  "displayName": "CodeAid",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onCommand:extension.analyzeCode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.analyzeProject",
        "title": "Analyze whole project for SOLID Violations"
      },
      {
        "command": "extension.analyzeFile",
        "title": "Analyze current file for SOLID Violations"
      }
    ],
    "languages": [
      {
        "id": "java",
        "extensions": [
          ".java"
        ],
        "aliases": [
          "Java"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/pdfkit": "^0.13.6",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@google-cloud/aiplatform": "^3.34.0",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.7.8",
    "pdfkit": "^0.15.1"
  }
}
